.nav{
  font-family: calibri;
  display: flex;
  padding: 15px 30px;
  overflow: hidden;
  background-color: #65ADF5;
  justify-content: space-between;
  height:70px;
  margin-top: -8px;
  margin-left: -8px;
  margin-right: -8px;
  opacity: 0.8;
  letter-spacing:1.5px;
}
.nav ul{
	  list-style-type: none;
	  margin-left: 5px;
}
 .nav ul a {
  display: block;
  color: white;
  text-align: center;
  padding: 10px 18px;
  text-decoration: none;
  font-size: 16px;
  margin-left: 5px;
  margin-right:5px;
}
.icon{
	padding: 10px;
	font-size: 38px;
	font-weight:800;
	cursor: pointer;
	color: white;
	margin-right: -50px;
	margin-left: -20px;
}
.nav li {
	margin: 0 2px;
	float: left;
}
 li a:hover {
  background-color: #08396A;
  border-radius: 20px;
}
.search-box{
	padding: 20px;
	display: flex;
	margin-right: -50px;
	height: 35px;
	line-height: 35px;
	
}
.search-box input{
	border: none;
	outline: none;
	background: white;
	height:100%;
	padding: 0 50px;
	font-size: 20px;
	width: 500px;
	text-align:center;
	border-radius: 20px;
}

.signup a{
	display: flex;
	background-color: #2664AB;
	border-radius: 5px;
}
.signup  a:hover{
	
	background-color: #08396A;
	border-radius: 5px;
}
.active a{
	display: flex;
	background-color: #08396A;
	border-radius: 20px;
}
body{
	background-image: url("../images/University.jpg");
	background-position: center;
	background-size: cover;
	position: relative;
	font-family: calibri;
	justify-content: center;
	background-repeat: no-repeat;
	display: block;
	padding: 0;
	margin: 0;
	overflow-x: hidden;
	width:100%;
	height:100%;
}
.logo{
	display: block;
	width:200px;
	height:150px;
	background-color: #fff;
	margin-top: 10px;
	margin-left: 10px;
	border-radius: 10px;
	opacity: 0.8;
}

.Notices{
	text-align :justify;
	background-color:#fff;
	letter-spacing: 1.5px;
}
.footer{
	font-family: calibri;
	padding:0px 30px;
	width: 100%;
	background-color: #65ADF5;
	display: flex;
	margin-left:0;
	overflow:hidden;
	height:190px;
	opacity:0.9;
}
.Notices p{
	margin-left: 12px;
	margin-right: 20px;
}
.Notices h1{
	padding: 10px;
	text-align: center;
	margin-left: 10px;
}
.Notices h2{
	margin-left: 10px;
}
.footer div{

	letter-spacing: 1px;
	
}
.col-1{
	width: 100%;
	margin-left: -35px;
}
.col-1 a{
	width: 20%;
	min-width:20%;
	display: block;
	text-decoration: none;
	color: black;
	background-color: white;
	box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
	padding: 10px 0px;
	text-align: center;
	margin-bottom: 2%;
	letter-spacing: 0;
	margin-left: 20px;
	margin-top: 10px;
}
.row-1{
	margin-bottom: 4%;
	margin-left: 40px;
}
.row-2{

	width: 100%;
	padding: 20px 20px;
	background-color: #89F9D1;
	height: 30px;
	margin-top: 54px;
	overflow: hidden;
}
.col-2{
	
	flex-grow: 2;
	width: 105%;
	display: block;
	background-color: #89F9D1;
	height: 190px;
	margin-top: 12px;
	overflow: hidden;
}

.fa{
 
  padding: 7.5px;
  font-size: 15px;
  width: 15px;
  text-align: center;
  text-decoration: none;
  border-radius: 50%;
  margin-left: 0%;
  margin-right: 2%;
  margin-top: 15px;
  overflow:hidden;
}
.fa:hover {
    opacity: 0.7;
}

.fa-facebook {
  background: #3B5998;
  color: white;
  margin-left: 500px;
}

.fa-twitter {
  background: #55ACEE;
  color: white;
}

.fa-google {
  background: #dd4b39;
  color: white;
}
.fa-instagram {
  background: #125688;
  color: white;
}
.fa-linkedin {
  background: #007bb5;
  color: white;
}
.qr-code{
	width: 90px;
	height: 90px;
	margin-bottom: 32px;
	margin-left:75%;
	overflow:hidden;
}
.imgarea{
	display: block;
	width: 100%;
	background-color: #65ADF5;
	overflow: hidden;
}
		
